    <command:details>
      <command:name>Out-Xlsx</command:name>
      <maml:description>
        <maml:para>Sends output to an Excel workbook</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para />
      </maml:copyright>
      <command:verb>Out</command:verb>
